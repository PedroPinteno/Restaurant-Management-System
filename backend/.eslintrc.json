{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "no-param-reassign": ["error", { "props": false }],
        "func-names": "off",
        "consistent-return": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
